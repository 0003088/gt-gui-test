# Find the Elektra Library
#
# ELEKTRA_INCLUDE_DIRS - where to find Elektra's Header files.
# ELEKTRA_LIBRARIES - the library to link against
# ELEKTRA_FOUND - always set to one when this file was found

set (Elektra_LIBRARY_DIRS "CMAKE_INSTALL_PREFIX@/lib")

set (Elektra_BUILD_SETTINGS_FILE "@CMAKE_INSTALL_PREFIX@/ElektraBuildSettings.cmake")

include ("@CMAKE_INSTALL_PREFIX@/ElektraLibraryDepends.cmake")

set (Elektra_USE_FILE "@CMAKE_INSTALL_PREFIX@/ElektraUse.cmake")

set (ELEKTRA_FOUND 1)

set (ELEKTRA_INCLUDE_DIRS "@CMAKE_INSTALL_PREFIX@/lib")

set (ELEKTRA_LIBRARIES "@ELEKTRA_LIBRARIES@")
