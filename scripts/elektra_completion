# Elektra completion
#
_kdb()
{
	local cur compstr

	COMPREPLY=()
	cur=${COMP_WORDS[COMP_CWORD]}
	if [ -z $cur ]; then
	    compstr=${COMP_WORDS[*]}
	else
	    compstr=$( command echo ${COMP_WORDS[*]} | sed "s/ $cur$//" )
	fi
	COMPREPLY=( $( compgen -W '$( command $compstr | sed s/\(.*\)// )'  -- $cur ) )
}
[ "${have:-}" ] && complete -F _kdb kdb

