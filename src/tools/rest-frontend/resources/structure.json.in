[
	{
		"name": "Documentation",
		"type": "submenu",
		"ref": "documentation",
		"children": [
			{
				"name": "Getting started",
				"type": "staticlist",
				"ref": "docgettingstarted",
				"children": [
					{
						"name": "Compiling",
						"type": "staticfile",
						"options": {
							"path": "doc/COMPILE.md"
						}
					},
					{
						"name": "Installation",
						"type": "staticfile",
						"options": {
							"path": "doc/INSTALL.md"
						}
					}
				]
			},
			{
				"name": "Tutorials",
				"type": "listfiles",
				"ref": "tutorials",
				"options": {
					"path": "doc/tutorials",
					"blacklist": ["CMakeLists.txt"]
				}
			},
			{
				"name": "Plugins",
				"type": "parsereadme",
				"ref": "plugins",
				"options": {
					"path": "src/plugins/README.md",
					"target_file": ["README.md", "README", "readme.md", "readme"],
					"parsing": {
						"start_regex": "# Plugins #",
						"stop_regex": "####### UNUSED ######",
						"section_regex": "## ([^#]+) ##",
						"entry_regex": "\\- \\[(.+)\\]\\(([^\\)]+)\\)(.*)"
					}
				}
			},
			{
				"name": "Tools",
				"type": "listdirs",
				"ref": "tools",
				"options": {
					"path": "src/tools",
					"target_file": ["README.md", "README", "readme.md", "readme"]
				}
			},
			{
				"name": "Manpages",
				"type": "listfiles",
				"ref": "manpages",
				"options": {
					"path": "doc/help",
					"blacklist": ["CMakeLists.txt"]
				}
			}
		]
	},
	{
		"name": "Development",
		"type": "submenu",
		"ref": "development",
		"children": [
			{
				"name": "Getting started",
